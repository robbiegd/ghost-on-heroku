<div class="members-cta members-cta-paid-members {{visibility}}">
	<h2 class="members-cta-text-color">{{t "This post is for paying subscribers only"}}</h2>
	{{#if @member}}
	<p class="members-cta-text-color">
		{{t "Upgrade your account to read the post and get access to the full library of posts for paying subscribers only."}}
	</p>
	<a class="members-cta-button global-button" href="{{@site.url}}/account/" class="members-cta-text-color">{{t "See plans"}}</a>
	{{else}}
	<p class="members-cta-text-color">
		{{t "Sign up now and upgrade your account to read the post and get access to the full library of posts for paying subscribers only."}}
	</p>
	<a class="members-cta-button global-button" href="{{@site.url}}/signup/">{{t "Sign up now"}}</a>
	<small class="members-cta-question members-cta-text-color global-question">{{t "Already have an account?"}} <a href="{{@site.url}}/signin/" class="members-cta-text-color">{{t "Sign in"}}</a></small>
	{{/if}}
</div>
